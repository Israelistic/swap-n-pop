{
  "name": "swap-n-pop",
  "productName": "swap-n-pop",
  "license": "MIT",
  "description": "Puzzle Game",
  "version": "0.4.0",
  "author": "Andrew Brown",
  "repository": {
    "url": "https://github.com/omenking/swap-n-pop"
  },
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-builder": "^19.46.9",
    "electron-webpack": "^1.11.0",
    "webpack": "^3.8.1",
    "chai": "^4.1.1",
    "docdash": "^0.4.0",
    "jsdoc": "^3.5.5",
    "mocha": "^3.5.0",
    "mock-require": "^2.0.2",
    "sinon": "^3.2.1"
  },
  "resolutions": {
      "webpack-sources": "1.0.1"
  },
  "build": {
    "appId": "com.swap-n-pop.app",
    "mac": {
      "category": "public.app-category.games",
      "publish": [
        "github"
      ],
      "icon": "./app/src/assets/icons/mac/icon.icns"
    },
    "win": {
      "target": "nsis",
      "publish": [
        "github"
      ],
      "icon": "./app/src/assets/icons/win/icon.ico"
    }
  },
  "scripts": {
    "dev": "electron-webpack dev",
    "build": "electron-webpack",
    "dist": "yarn build && electron-builder --dir"
  },
  "postinstall": "electron-builder install-app-deps",
  "dependencies": {
    "electron-reload": "^1.2.2",
    "electron-store": "^1.3.0",
    "glob": "^7.1.2",
    "keycode": "^2.1.9",
    "mithril": "^1.1.5",
    "mkdir-recursive": "^0.3.0",
    "phaser-ce": "^2.9.2",
    "seedrandom": "^2.4.3",
    "shuffle-seed": "^1.1.6",
    "strftime": "^0.10.0"
  }
}
